{
  "name": "one-oracle",
  "version": "1.0.0",
  "description": "Vesper oracle contracts",
  "license": "MIT",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "solhint": "solhint './contracts/**/*.sol'",
    "solhint:fix": "prettier --write 'contracts/**/*.sol'",
    "lint": "eslint --cache ./test/*",
    "lint:fix": "eslint --cache --fix ./test --ext .ts",
    "hardhat": "hardhat",
    "clean": "hardhat clean",
    "compile": "hardhat compile",
    "coverage": "./scripts/coverage-all-chains.sh",
    "deploy": "hardhat deploy",
    "pretest": "hardhat typechain",
    "test": "hardhat test",
    "test:mainnet": "cp .github/mainnet.env.properties .env && npm t -- --grep @mainnet",
    "test:arbitrum": "cp .github/arbitrum.env.properties .env && npm t -- --grep @arbitrum",
    "test:polygon": "cp .github/polygon.env.properties .env && npm t -- --grep @polygon",
    "test:avalanche": "cp .github/avalanche.env.properties .env && npm t -- --grep @avalanche",
    "test:bsc": "cp .github/bsc.env.properties .env && npm t -- --grep @bsc",
    "test:mumbai": "cp .github/mumbai.env.properties .env && npm t -- --grep @mumbai",
    "test:all": "./scripts/test-all-chains.sh",
    "verify": "hardhat etherscan-verify",
    "size-contract": "hardhat size-contracts",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@chainlink/contracts": "^0.4.1",
    "@defi-wonderland/smock": "^2.0.7",
    "@ethersproject/hardware-wallets": "^5.6.1",
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.6.0",
    "@prb/math": "^2.5.0",
    "@typechain/ethers-v5": "^10.0.0",
    "@typechain/hardhat": "^6.0.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@umb-network/toolbox": "^5.9.1",
    "@uniswap/lib": "^1.1.4",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "@uniswap/v3-periphery": "^1.4.1",
    "chai": "^4.3.6",
    "chai-almost": "^1.0.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.16.0",
    "eslint-config-bloq": "^3.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.2",
    "eslint-plugin-mocha": "^10.0.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-require-path-exists": "^1.1.9",
    "ethereum-waffle": "^3.4.4",
    "ethereumjs-util": "^7.1.4",
    "ethers": "^5.6.8",
    "hardhat": "^2.9.7",
    "hardhat-contract-sizer": "^2.5.1",
    "hardhat-deploy": "^0.11.10",
    "hardhat-gas-reporter": "^1.0.8",
    "hardhat-log-remover": "^2.0.2",
    "hardhat-spdx-license-identifier": "^2.0.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "lodash": "^4.17.21",
    "p-series": "^3.0.0",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solc": "0.8.13",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.21",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.8.0",
    "typechain": "^8.0.0",
    "typescript": "^4.7.2"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=7.0.0"
  }
}
